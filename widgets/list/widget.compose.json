        "en": "List",
        "nl": "Lijst"
    },
    "height": 120,
    "devices": {
        "type": "app",
        "singular": true,
        "filter": {}
    },
    "settings": [],
    "api": {
        "addItem": {
            "method": "POST",
            "path": "/:deviceId/items"
        },
        "get": {
            "method": "GET",
            "path": "/:deviceId"
        },
        "getItem": {
            "method": "GET",
